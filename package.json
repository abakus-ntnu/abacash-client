    "electron-packager": "8.1.0",
    "electron-rebuild": "1.2.1",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-flowtype": "^2.19.0",
